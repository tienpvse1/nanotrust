{
  "name": "nanotrust",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "console": "cd hasura && hasura console",
    "build:all": "nx run-many --target=build --all",
    "migrate": "cd hasura && hasura migrate apply && hasura metadata apply",
    "compose-up:local": "sudo docker-compose -f docker-compose.local.yml up -d",
    "compose-down:local": "docker-compose -f docker-compose.local.yml down",
    "compose-up:prod": "sudo docker-compose -f docker-compose.production.yml up -d",
    "compose-down:prod": "docker-compose -f docker-compose.production.yml down",
    "compose-up:dev": "sudo docker-compose -f docker-compose.development.yml up -d",
    "compose-down:dev": "docker-compose -f docker-compose.development.yml down"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "dotenv": "^16.0.1",
    "express": "4.17.2",
    "http-status-codes": "^2.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/cli": "14.1.7",
    "@nrwl/eslint-plugin-nx": "14.1.7",
    "@nrwl/express": "^14.1.7",
    "@nrwl/jest": "14.1.7",
    "@nrwl/linter": "14.1.7",
    "@nrwl/nest": "^14.1.7",
    "@nrwl/node": "14.1.7",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "14.1.7",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "~5.18.0",
    "@typescript-eslint/parser": "~5.18.0",
    "eslint": "~8.12.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "27.5.1",
    "nx": "14.1.7",
    "prettier": "^2.5.1",
    "ts-jest": "27.1.4",
    "ts-node": "9.1.1",
    "typescript": "~4.6.2"
  }
}
